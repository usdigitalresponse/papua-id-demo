%ul.none
  - if @applicant.decision_response['raw_responses']['Socure 30'][0]['kyc']['fieldValidations']['mobileNumber'] == 0.99
    %li
      = decision_icon('Approved')
      Socure 30
      = t('admin.applicants.show.matches.phone')
  - if @applicant.decision_response['raw_responses']['White Pages Pro'][0]['phone_checks']['is_valid']
    %li
      = decision_icon('Approved')
      WPP
      = t('admin.applicants.show.matches.phone')
  -begin
    - if @applicant.decision_response['raw_responses']['Lexis Nexis Instant ID'][0]['response']['Result']['VerifiedInput']['HomePhone'].present?
      %li
        = decision_icon('Approved')
        LexisNexis
        = t('admin.applicants.show.matches.phone')
  - rescue
    %li
      = decision_icon('Denied')
      LexisNexis
      = t('admin.applicants.show.error.phone')
