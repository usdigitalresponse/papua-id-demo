%ul.none
  - if @applicant.decision_response['raw_responses']['Socure 30'][0]['kyc']['fieldValidations']['firstName'] == 0.99
    %li
      = decision_icon('Approved')
      Socure 30
      = t('admin.applicants.show.matches.first_name')
  - unless @applicant.decision_response['raw_responses']['White Pages Pro'][0]['name_checks']['fake_name']
    %li
      = decision_icon('Approved')
      WPP
      = t('admin.applicants.show.matches.first_name')
  - begin
    - if @applicant.decision_response['raw_responses']['Lexis Nexis Instant ID'][0]['response']['Result']['VerifiedInput']['Name']['First'].present?
      %li
        = decision_icon('Approved')
        LexisNexis
        = t('admin.applicants.show.matches.first_name')
  - rescue
    %li
      = decision_icon('Denied')
      LexisNexis
      = t('admin.applicants.show.error.first_name')
