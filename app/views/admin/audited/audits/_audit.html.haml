%tr
  %td= audit.created_at.to_datetime.strftime('%F %T')
  %td= 'Someone'
  %td= audit.action
  %td
    %ul
      - audit.audited_changes.each do |property, values|
        %li= t('claim.history.change', property: t("claim.properties.#{property}"), initial_value: t_enum(Applicant, property, values[0]), final_value: t_enum(Applicant, property, values[1]))
